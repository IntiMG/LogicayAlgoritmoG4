Algoritmo Problema1
	//definir las variables
	Definir num1 Como Entero;
	Definir num2 Como Entero;
	Definir sumar como entero;
	//Proceso 
	Escribir "Ingrese un numero: ";
	Leer num1;
	Escribir "Ingrese otro numero: ";
	Leer num2;
	sumar <- num1 + num2;
	Escribir "La suma es: ", sumar;
